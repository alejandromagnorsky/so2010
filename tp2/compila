cd ./bin
rm -rf *
nasm -f aout ../src/libasm.asm -o libasm.o
nasm -f aout ../src/kstart.asm -o kstart.o
nasm -f aout ../src/loader.asm -o kstart.o
gcc -c ../src/kernel.c -o kernel.o -fno-builtin
gcc -c ../src/libc.c  -o libc.o -fno-builtin
gcc -c ../src/io.c  -o io.o -fno-builtin
gcc -c ../src/string.c  -o string.o -fno-builtin
gcc -c ../src/klib.c  -o klib.o -fno-builtin
gcc -c ../src/keyboard.c -o keyboard.o -fno-builtin
gcc -c ../src/shell.c -o shell.o -fno-builtin
gcc -c ../src/screen.c -o screen.o -fno-builtin
gcc -c ../src/cpuid.c -o cpuid.o -fno-builtin
gcc -c ../src/gralprotection.c -o gralprotection.o -fno-builtin
gcc -c ../src/mem.c -o mem.o -fno-builtin
gcc -c ../src/task.c -o task.o -fno-builtin
ld -T ../src/link.ld -o kernel.bin kernel.o kstart.o libc.o libasm.o io.o string.o klib.o keyboard.o shell.o screen.o cpuid.o gralprotection.o task.o mem.o
